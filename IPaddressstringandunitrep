public class Solution {

    private static uint convertIpStringToUint(string baseIP)
    {
        string[] baseIpParts = baseIP.Split('.');
        uint ipAddress = 0;
        for(int i = 0; i<3;i++)
        {
            ipAddress |= uint.Parse(baseIpParts[i]) << (8 * (3-i));
        }
        return ipAddress;
    }
    
    private static string convertIpUintToString(uint ipAddr)
    {
        StringBuilder sb = new StringBuilder();
        for(int i=3;i>=0;i--)
        {
            uint partNumber = (ipAddr >> (8 * i)) & 0xFF;
            sb.Append(partNumber.ToString());
            if(i!=0) sb.Append('.');
        }
        return sb.ToString();
    }

    public string DefangIPaddr(string address) {
        // Start - 10 06
        // End - 10 48
        string ip = "256.24.31.2";
        Console.WriteLine(convertIpUintToString(convertIpStringToUint(ip)));
        return ip;
    }
}
